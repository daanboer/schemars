{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Parent",
  "type": "object",
  "required": [
    "child"
  ],
  "properties": {
    "child": {
      "type": "object",
      "required": [
        "name", 
        "age"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "age": {
          "type": "integer",
          "minimum": 0,
          "format": "uint16"
        }
      }
    }
  }
}